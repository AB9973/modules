{
    "name": "pingpong/modules",
    "description": "Laravel Modules",
    "keywords": [
        "modules",
        "laravel",
        "pingpong",
        "hmvc",
        "module"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Pingpong Labs",
            "email": "pingpong.labs@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~4|~5",
        "illuminate/filesystem": "~4|~5",
        "illuminate/translation": "~4|~5",
        "illuminate/view": "~4|~5",
        "illuminate/config": "~4|~5",
        "illuminate/html": "~4|~5",
        "illuminate/events": "~4|~5",
        "illuminate/routing": "~4|~5",
        "pingpong/generators": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Pingpong\\Modules\\": "src/Pingpong/Modules"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev",
            "dev-develop": "1.2-dev"
        }
    },
    "minimum-stability": "stable"
}
