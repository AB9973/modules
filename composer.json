{
    "name": "pingpong/modules",
    "description": "Laravel 4 - Simple Modules",
    "keywords":["modules", "laravel", "pingpong", "hmvc"],
    "license":"MIT",
    "authors": [
        {
            "name": "Pingpong Labs",
            "email": "pingpong.labs@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.*",
        "illuminate/filesystem": "4.*",
        "illuminate/translation": "4.*",
        "illuminate/view": "4.*",
        "illuminate/config": "4.*",
        "illuminate/html": "4.*",
        "illuminate/routing": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Pingpong\\Modules\\": "src/Pingpong/Modules"
        }
    },
    "minimum-stability": "stable"
}
